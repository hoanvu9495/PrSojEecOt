                                        alert(xhr.responseText);
                                    }
                                });
                            } else {
                                $("#trangthai_" + thutu).remove();
                                $("#SoLuongTrangThai").val(parseInt(SoLuongTrangThai) - 1);
                                var i = 1;
                                $(".tr_trangthai").each(function () {
                                    $(this).find("td").first().html(i);
                                    i++;
                                });
                            }
                        }
                    },
