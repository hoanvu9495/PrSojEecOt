    function ThietLap(thutu, id) {
        var wf_id = $("#WF_ID").val();
        $.ajax({
            url: '@Url.Action("Setting", "WorkFlowState")',
            type: 'post',
            cache: false,
            data: { id: id, wf_id: wf_id },
            success: function (data) {
                $("#setting_content").html(data);
                $("#setting_dialog").modal("show");
            },
            error: function (xhr) {
