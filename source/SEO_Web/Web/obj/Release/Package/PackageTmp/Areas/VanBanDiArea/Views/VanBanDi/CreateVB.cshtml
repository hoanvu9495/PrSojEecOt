<style type="text/css">
    .nhieunguoiky {
        display: none;
    }

    #vb_di_tabs section {
        height: 100px;
    }

    #tab_thongtinchinh, #tab_quytrinhxuly {
        overflow: auto;
        height: 34em;
        padding-bottom: 20px;
    }

    #tab_noidungvanban {
        padding-bottom: 20px;
    }

    #tab_butphelanhdao {
        padding-bottom: 20px;
    }

    #tab_donvinhan_theodoi_vb {
        overflow: auto;
        height: 34em;
        padding-bottom: 20px;
    }

    #frmVanBanDiCreate label {
        font-weight: normal !important;
    }

    #TRICHYEU {
        width: 100%;
        height: 40px;
        margin-left: 10px;
        font-size: 13px;
    }

    #button--add-new-vanbandi {
        margin-right: 10px;
    }

    #block--left-element-1 {
        background: none;
        border: 0;
    }

    #list--admin-menu {
        z-index: 10;
    }

    #list--has-child-1, #list--has-child-2 {
        z-index: 10;
    }

    #vb_di_tabs {
        padding-bottom: 10px;
    }

    .required-mark {
        color: red;
    }
