        margin-left: 10px;
    }

    #THOIHAN_HOIBAO_FORMAT {
        padding-left: 115px;
    }

    #NGAY_XULY {
        width: 233px;
        margin-left: 10px;
    }

    #NOIDUNGVANBAN {
        width: 700px;
        height: 280px;
        margin-left: 10px;
    }

    #LOAIVANBAN_ID ~ .ms-parent {
        margin-left: 10px;
    }
    

    #DIEUKHOANLIENQUANCONGVIEC, #GHICHU {
        width: 700px;
        height: 40px;
        margin-left: 10px;
    }

    #LANHDAOBUTPHE {
        width: 250px;
    }

    #CHONLANHDAO,
    #CHONNGUOITHEODOICHINH {
        margin: 0 10px 0 5px;
    }
    #XOALANHDAO,#CHONNGUOIKY,#CHONNGUOISOAN
    #XOANGUOITHEODOICHINH,#XOANGUOIKY,#XOANGUOISOAN{
        margin-left:10px;
    }
    #YKIENCHIDAO{
        width:600px;
        height:200px; 
        margin-left:10px
    }
    #title--donvitheodoi{
        margin-bottom:10px;
        text-transform:none;
    }
    #cell--donvitheodoi-label{
        width:200px;
    }
    #block--DONVITHEODOICHINH_ID{
        width:233px;
    }
    #NGUOITHEODOICHINH{
        width:350px;
    }
    #title--DONVINHANVANBAN{
        margin-bottom:10px;
        text-transform:none;
    }
