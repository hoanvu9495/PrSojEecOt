@using StackExchange.Profiling;
@{
    UserInfoBO user = ((UserInfoBO)SessionManager.GetUserInfo());
    if (user == null)
    {
        user = new UserInfoBO();
    }

    MvcHtmlString token = null;
    try
    {
        token = Html.AntiForgeryToken();
    }
    catch { }
}
